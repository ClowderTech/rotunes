{
	"type": "module",
	"imports": {
		"@discordjs/builders": "npm:@discordjs/builders@^1.10.0",
		"@discordjs/voice": "npm:@discordjs/voice@^0.18.0",
		"discord-api-types": "npm:discord-api-types@^0.37.115",
		"discord.js": "npm:discord.js@^14.17.3",
		"domutils": "npm:domutils@^3.2.2",
		"mongodb": "npm:mongodb@^6.12.0",
		"moonlink.js": "npm:moonlink.js@^4.2.1",
		"ollama": "npm:ollama@^0.5.11",
		"jsdom": "npm:jsdom@^25.0.1",
		"openblox": "npm:openblox@^1.0.59",
		"sharp": "npm:sharp@^0.33.5",
		"texsvg": "npm:texsvg@^2.2.2"
	},
	"tasks": {
		"envstart": "deno run --env-file --allow-env --allow-read --allow-write --allow-net --allow-sys --allow-run --allow-ffi src/index.ts",
		"start": "deno run --allow-env --allow-read --allow-write --allow-net --allow-sys --allow-run --allow-ffi src/index.ts",
		"test": "deno test",
		"lint": "deno lint && deno fmt --indent-width 4 --use-tabs --check && deno check src/*",
		"fixlint": "deno fmt --indent-width 4 --use-tabs",
		"updateall": "deno outdated --update --latest"
	},
	"nodeModulesDir": "auto",
	"compilerOptions": {
		"lib": ["DOM", "ESNext", "deno.ns"]
	}
}
